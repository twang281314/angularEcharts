{
  "env": {
    "browser": true,
    "jquery": true,
    "jasmine": true,
    "protractor": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "angular"
  ],
  "globals": {
    "angular": true,
    "echarts": true,
    "module": true,
    "inject": true
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}